<?xml version="1.0" encoding="utf-8"?>
<!--
表示一个名片模板 Business Card Template
guid 模板唯一标记
name 模板名称
size 模板宽度和高度(px)
-->
<BCTemp guid="" name="" size="" >
  <!--正面-->
  <Front colors="">
    <!--
    模板背景元素
    color R(0-255)G(0-255)B(0-255)A(0-100)
    -->
    <BG color="">
      <!--
      表示背景中的一个图形(可选)
      id 图形索引值
      rect 图形相对于名片画布的矩形范围
      visible 可见性
      depth 深度
      edit 表示图形是否允许编辑
      -->
      <Symbol id="" rect="" depth="" edit="" />
    </BG>

    <!--Logo的默认放置位置-->
    <Logo rect="" />

    <!--文字集合-->
    <Texts>
      <Text rect="" font="" size="" style="" ><![CDATA[文字内容]]></Text>
    </Texts>
  </Front>

  <!--背面-->
  <Back colors="">
    <BG color="">
      <!--
    表示背景中的一个图形(可选)
    id 图形索引值
    rect 图形相对于名片画布的矩形范围
    visible 可见性
    depth 深度
    edit 表示图形是否允许编辑
    -->
      <Symbol id="" rect="" depth="" edit="" />
    </BG>

    <!--Logo的放置位置-->
    <Logo rect="" />

    <!--文字集合-->
    <Texts>
      <Text rect="" font="" size="" style="" ><![CDATA[文字内容]]></Text>
    </Texts>
  </Back>
  
  <!--轮廓集合-->
  <Glyphs>
    <!--
    表示一个图形的轮廓
    id 相关的图形索引值
    type 表示轮廓是图形还是单字
    fc 轮廓的填充颜色
    b 标记轮廓有无线条
    bc 线条颜色
    rect 轮廓相对于父级的矩形区域
    -->
    <Glyph id="" fc="" b="" bc="" rect="">
      <!--
      表示轮廓的一个线段
      t 线段类型。0-起点，1-直线，2-曲线
      p 点坐标。
        t=0或1 表示起点或直线终点
        t=2|3 表示(图形)二次贝塞尔曲线或(文字)三次贝塞尔曲线的控制点和终点
      -->
      <S t="" p="" />
    </Glyph>
  </Glyphs>
</BCTemp>